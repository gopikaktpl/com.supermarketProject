<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Cross Browser Test Suite" thread-count="5" parallel="tests">
	<test name="Chrome Browser Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="executeClass.LogInPageExecution"></class>
			<class name="executeClass.DashboardExecution"></class>
			<class name="executeClass.ManageProductExecution"></class>
			<class name="executeClass.ExpenceCategoryPageExecution"></class>
			<class name="executeClass.AdminUsersPageExecution"></class>
			<class name="executeClass.CategoryPageExecution"></class>

		</classes>
	</test>
	
	<test name="Edge Browser Test">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="executeClass.LogInPageExecution"></class>
			<class name="executeClass.DashboardExecution"></class>
			<class name="executeClass.ManageProductExecution"></class>
			<class name="executeClass.ExpenceCategoryPageExecution"></class>
			<class name="executeClass.AdminUsersPageExecution"></class>
			<class name="executeClass.CategoryPageExecution"></class>

		</classes>
	</test>
</suite>
